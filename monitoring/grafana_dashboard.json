{
  "title": "Recommender API",
  "panels": [
    {
      "type": "graph",
      "title": "Requests/s",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m]))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Error rate",
      "targets": [
        {
          "expr": "sum(rate(http_errors_total[5m])) / sum(rate(http_requests_total[5m]))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Latency p95",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_latency_seconds_bucket[5m])) by (le))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Uptime",
      "targets": [
        {
          "expr": "service_uptime_seconds"
        }
      ]
    }
  ],
  "schemaVersion": 27,
  "version": 1
}